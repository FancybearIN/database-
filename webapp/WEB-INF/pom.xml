<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>SqlQueryServlet</servlet-name>
        <servlet-class>com.example.SqlQueryServlet</servlet-class> 
    </servlet>

    <servlet-mapping>
        <servlet-name>SqlQueryServlet</servlet-name>
        <url-pattern>/SqlQueryServlet</url-pattern> 
    </servlet-mapping>

</web-app>
